{
    "manifest_version": 2,
    "name": "Block YouTube",
    "version": "1.0",
    "description": "Blocks access to YouTube",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "icons": {
      "48": "icon.png"
    }
  }
